	file = {Napolitano et al. - 2002 - NGC 1399 A complex dynamical case.pdf:/home/colin/Zotero/storage/MEYYW437/Napolitano et al. - 2002 - NGC 1399 A complex dynamical case.pdf:application/pdf}
}

