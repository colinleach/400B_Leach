	note = {Publisher: EDP Sciences},
	pages = {A62},
	file = {Full Text PDF:/home/colin/Zotero/storage/VNHWVXY7/Tsatsi et al. - 2017 - CALIFA reveals prolate rotation in massive early-t.pdf:application/pdf;Snapshot:/home/colin/Zotero/storage/JPS5G782/aa30218-16.html:text/html}
}

@article{wang_orbit_2019,
	title = {Orbit properties of massive prolate galaxies in the {Illustris} simulation},
