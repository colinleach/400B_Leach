	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Binney, J.},
	month = may,
	year = {1978},
	keywords = {Astronomical Models, Cosmology, Elliptical Galaxies, Ellipticity, Galactic Rotation, Mathematical Models, Prolate Spheroids, Virial Theorem},
	pages = {501--514},
	file = {Full Text PDF:/home/colin/Zotero/storage/FB967JYT/Binney - 1978 - On the rotation of elliptical galaxies.pdf:application/pdf}
}

